{
  "name": "mofier",
  "version": "0.2.27",
  "description": "核心包",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://mofier@bitbucket.org/mofier/nuwa.git"
  },
  "author": "Mofier <mrmofier@gmail.com>",
  "license": "MIT",
  "pre-commit": {
    "run": [
    ]
  },
  "scripts": {
    "lint:js": "eslint src",
    "lint": "npm run lint:js",
    "test": "babel-node test/runner.js",
    "build": "rm -rf lib && (babel src --presets babel-preset-es2015 --out-dir lib || exit 1) && cp src/fetch/package.json lib/fetch/package.json && cp package.json lib/package.json",
    "prepublish": "(npm run build || exit 1) && (npm test || exit 1) "
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "bluebird": "^3.5.0",
    "debug": "^3.1.0",
    "generic-pool": "^3.2.0",
    "influx": "^5.0.7",
    "mysql": "^2.13.0",
    "node-fetch": "^1.6.3",
    "pretty-error": "^2.1.0",
    "redis": "^3.1.1",
    "redlock": "^2.1.0",
    "rxjs": "^5.5.5",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-template": "^6.24.1",
    "babel-types": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^3.3.0",
    "pre-commit": "^1.2.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "globals": {
      "__DEV__": true
    },
    "env": {
      "browser": true
    },
    "rules": {
      "indent": [
        0,
        4
      ],
      "semi": 0,
      "radix": 0,
      "new-cap": 0,
      "one-var": 0,
      "no-empty": 1,
      "quotes": 1,
      "dot-notation": 0,
      "no-case-declarations": 0,
      "react/jsx-boolean-value": 0,
      "react/forbid-prop-types": 0,
      "no-multi-assign": 0,
      "padded-blocks": 0,
      "react/self-closing-comp": 0,
      "react/no-string-refs": 1,
      "no-script-url": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-first-prop-new-line": 0,
      "react/jsx-curly-spacing": 0,
      "react/jsx-indent-props": 0,
      "react/jsx-closing-bracket-location": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/img-has-alt": 0,
      "react/prop-types": 0,
      "func-names": 0,
      "guard-for-in": 1,
      "no-useless-escape": 1,
      "react/jsx-indent": 0,
      "no-restricted-syntax": 1,
      "css-modules/no-unused-class": 0,
      "prefer-spread": 0,
      "prefer-const": 0,
      "semi-spacing": 0,
      "max-len": 0,
      "comma-dangle": 0,
      "operator-assignment": 0,
      "space-before-function-paren": 0,
      "no-inner-declarations": 0,
      "one-var-declaration-per-line": 0,
      "object-property-newline": 0,
      "arrow-parens": 0,
      "no-unused-expressions": 0,
      "no-use-before-define": 0,
      "no-console": 0,
      "react/no-array-index-key": 0,
      "no-param-reassign": 0,
      "no-shadow": 0,
      "no-plusplus": 0,
      "no-confusing-arrow": 0,
      "comma-spacing": 0,
      "object-curly-spacing": 0,
      "arrow-spacing": 0,
      "space-before-blocks": 0,
      "no-unused-vars": 0,
      "import/newline-after-import": 0,
      "spaced-comment": 0,
      "import/first": 0,
      "no-underscore-dangle": 0,
      "no-extra-semi": 0,
      "keyword-spacing": 0,
      "no-else-return": 0,
      "space-in-parens": 0,
      "arrow-body-style": 0,
      "camelcase": 0,
      "no-return-await": 0,
      "key-spacing": 0,
      "react/sort-comp": 0,
      "global-require": 0,
      "space-infix-ops": 0,
      "object-curly-newline": 0,
      "template-curly-spacing": 0,
      "consistent-return": 0,
      "array-bracket-spacing": 0,
      "computed-property-spacing": 0,
      "import/extensions": "off",
      "newline-per-chained-call": 0,
      "import/prefer-default-export": 0,
      "import/no-extraneous-dependencies": "off",
      "react/jsx-filename-extension": "off",
      "react/prefer-stateless-function": "off"
    }
  },
  "homepage": "https://bitbucket.org/mofier/mofier#readme",
  "directories": {
    "test": "test"
  }
}
